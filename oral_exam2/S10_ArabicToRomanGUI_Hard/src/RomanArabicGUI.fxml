<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RomanArabicConversionGui">
   <children>
      <GridPane layoutY="123.0" prefHeight="277.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="arabicExplanation" disable="true" prefHeight="157.0" prefWidth="300.0" text="Arabic Numerals:">
               <font>
                  <Font size="30.0" />
               </font>
            </TextField>
            <TextField fx:id="arabicNumberField" onAction="#arabicToRoman" prefHeight="157.0" prefWidth="300.0" GridPane.columnIndex="1">
               <font>
                  <Font size="24.0" />
               </font>
            </TextField>
            <TextField fx:id="romanExplanation" disable="true" prefHeight="157.0" prefWidth="300.0" text="Roman Numerals:" GridPane.rowIndex="1">
               <font>
                  <Font size="30.0" />
               </font>
            </TextField>
            <TextField fx:id="romanNumberField" onAction="#romanToArabic" prefHeight="157.0" prefWidth="300.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <TextField fx:id="title" disable="true" prefHeight="120.0" prefWidth="600.0">
         <font>
            <Font size="36.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
